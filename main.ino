#include "Led.h"  // Led outputs

/**************************************************************************
Declaration for LED modules
**************************************************************************/
#define LED_1_PIN 7
#define LED_2_PIN 6
#define LED_3_PIN 5

Led led1(LED_1_PIN);
Led led2(LED_2_PIN);
Led led3(LED_3_PIN);

/**************************************************************************
SETUP
**************************************************************************/
void setup() {
}

/**************************************************************************
MAIN LOOP
**************************************************************************/
void loop() {
    led1.shift();
    delay(200);
    led2.shift();
    delay(200);
    led3.shift();
    delay(200);
}
